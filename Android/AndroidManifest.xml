<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.asg"
    android:versionCode="1"
    android:versionName="1.1" >
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" >
    </uses-permission>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" >
    </uses-permission>
    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="18" />
    <application 
        android:allowBackup="true"
        android:name="ASGApplication"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.Holo.Light.DarkActionBar"  >
        <activity
            android:name="com.example.asg.view.MainActivity"
            android:label="@string/title_activity_main" />
        <activity
            android:name="com.example.asg.view.ConnectingActivity"
            android:label="@string/title_activity_connecting" />
        <activity
            android:name="com.example.asg.view.MenuActivity"
            android:label="@string/title_activity_menu" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.asg.view.PasswdActivity"
            android:label="@string/title_activity_passwd" />
        <activity
            android:name="com.example.asg.view.ScanActivity"
            android:label="@string/title_activity_scan" />
        <activity
            android:name="com.example.asg.view.ModifyParaActivity"
            android:label="@string/title_activity_modify_para" />
        <activity
            android:name="com.example.asg.view.GEPasswdActivity"
            android:label="@string/title_activity_gepasswd"/>  
        <activity
            android:name="com.example.asg.FatherActivity"
            android:label="@string/title_activity_father" />
        <activity
            android:name="com.example.asg.view.LoadCheckActivity"
            android:label="@string/title_activity_load_check" />
        <activity
            android:name="com.example.asg.view.ModifySQLDataActivity"
            android:label="@string/title_activity_modify_sqldata" />
        <activity
            android:name="com.example.asg.view.ShowSQLActivity"
            android:label="@string/title_activity_show_sql" />
    </application>
</manifest>